{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "global-require": 0,
    "consistent-return": "off",
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0
  },
  "globals": {
    "localStorage": true,
    "fetch": true,
    "document": true
  }
}