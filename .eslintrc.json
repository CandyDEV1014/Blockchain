{
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
        "no-console": 0,
        "class-methods-use-this": 1,
        "no-dupe-keys": 1,
        "no-underscore-dangle": 1
   
    },
    "globals": {
      "localStorage": true,
      "fetch": true,
      "document": true
    }
  }

